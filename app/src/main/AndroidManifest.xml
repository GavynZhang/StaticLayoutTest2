<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.gavynzhang.staticlayouttest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.gavynzhang.staticlayouttest.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gavynzhang.staticlayouttest.ui.StaticLayoutUI"
            android:label="@string/title_activity_static_layout_ui" >
        </activity>
        <activity
            android:name="com.gavynzhang.staticlayouttest.ui.NormalLayoutUI"
            android:label="@string/title_activity_normal_layout_ui" >
        </activity>
        <activity
            android:name="com.gavynzhang.staticlayouttest.ui.StaticLongStringUI"
            android:label="@string/title_activity_static_long_string_ui" >
        </activity>
        <activity
            android:name="com.gavynzhang.staticlayouttest.ui.NormalLongStringUI"
            android:label="@string/title_activity_normal_long_string_ui" >
        </activity>
    </application>

</manifest>